<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="sgeede_stock_warehouse_inherit_form_view" model="ir.ui.view">
        <field name="name">sgeede.stock.warehouse.inherit.form.view</field>
        <field name="model">stock.warehouse</field>
        <field name="inherit_id" ref="stock.view_warehouse"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="before">
                <group>
                    <separator string="Permission to Send and Receive Products" colspan="4"/>
                    <field name="user_ids" nolabel="1" colspan="4">
                        <tree>
                            <field name="name"/>
                        </tree>
                    </field>
                </group>
            </xpath>
        </field>
    </record>
    <record model="ir.ui.view" id="view_invoice_category_custom">
        <field name="name">account.invoice.supplier.form</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='ref']" position="after">
                <field name="vendor_ref" string="Vendor Ref"/>
            </xpath>
        </field>
    </record>
    <record model="ir.ui.view" id="view_invoice_category_custom_tree">
        <field name="name">account.invoice.supplier.form</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_invoice_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='state']" position="after">
                <field name="vendor_ref" string="Vendor Ref"/>
            </xpath>
        </field>
    </record>
</odoo>